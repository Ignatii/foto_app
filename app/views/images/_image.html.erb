<div class="col-sm-6 col-md-4">
  <div class="thumbnail">
    <%= link_to image_tag(image.image.thumb.url, :alt => image[:id]), {:controller => 'images', :action => 'show', :id => image[:id]},class:"thumbnail" %>
    <div class="caption">
      <h3>Votes:<%= image.score%>
      <% unless Identity.where(user_id: current_user.id, provider: 'facebook').first %>
        <%= link_to @str,  title: 'Share Me!', class:"btn btn-social-icon btn-xs btn-facebook share_fb" ,target:"_blank", style:"color:white;", id: "share_fb#{image.id}" do %>
  <i class="fa fa-facebook" data-content="Share Me!"></i>
<% end %>
<% end %>
      </h3>
      <% if current_user %>
       <p>
         <%= link_to "Upvote", like_image_path(image), method: :put , class: "btn btn-xs btn-success center-block" %>
         <%= link_to "Downvote", dislike_image_path(image), method: :put , class: "btn btn-xs btn-danger center-block" %>
       </p>
     <% end %>
     </div>
   </div>
</div>  
<script type="text/javascript">
  $(document).on('click', '#<%= "share_fb#{image.id}"%>', ( -> alert('test')))
</script>>